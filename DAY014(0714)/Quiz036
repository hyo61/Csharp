using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace Quiz01
{
    internal class Program
    {
        static void Main(string[] args)
        {
            while(true)
            {
                Console.Write("입력 : ");
                int n = int.Parse(Console.ReadLine());
                switch(n)
                {
                    case 1:
                        Console.WriteLine("enjoy");
                        break;
                    case 2:
                        Console.WriteLine("oops");
                        break;
                    case 3:
                        Console.WriteLine("enjoy");
                        break;
                    case 4:
                        Console.WriteLine("oops");
                        break;
                    case 5:
                        Console.WriteLine("enjoy");
                        break;
                    case 6:
                        Console.WriteLine("oops");
                        break;
                    case 7:
                        Console.WriteLine("oops");
                        break;
                }
            }
        }
    }
}
