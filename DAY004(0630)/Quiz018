using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace quiz8
{
    internal class Program
    {
        static void Main(string[] args)
        {
            Random r = new Random();
            int num2;
            Console.Write("로또 번호 : ");
            for(int i =  0; i <= 5;  i++) 
            { 
                num2 = r.Next(1, 45);
                Console.Write(num2 + " ");
            }
            Console.WriteLine();
            Console.WriteLine("보너스 번호 : "+ r.Next(1, 45));
        }
    }
}
