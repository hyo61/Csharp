using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp2Loop02
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int i = 0;

            while(i<5)
            {
                Console.WriteLine(i++); //01234 후치연산자,++i 12345 전치연산자
                Console.WriteLine("i : " + i);
            }
        }
    }
}
