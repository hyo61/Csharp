using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace cat
{
    class CAT
    {
        //필드, 멤버변수
        public string Name;
        public string Color;
    }
    class Program
    {
        static void Main(string[] args)
        {
            CAT kitty = new CAT(); //객체생성
            kitty.Color = "하얀색";
            kitty.Name = "키티";
            Console.WriteLine(kitty.Color + " " + kitty.Name);
        }
    }
}
