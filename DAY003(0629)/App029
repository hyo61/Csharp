using System;



namespace Quiz011

{

    internal class Program

    {

        static void Main(string[] args)

        {

            int[] arr = new int[10];

            //입출력 같이

            for (int i = 0, cnt = 70; i < arr.Length; i++, cnt -= 7)

            {

                arr[i] = cnt;

                Console.WriteLine(arr[i] + " ");
            }
        }
    }
}
