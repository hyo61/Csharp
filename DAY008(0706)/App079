using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApp1
{
    internal class Program
    {
        static void Main(string[] args)
        {
            string[] arr = { "안녕", "하세요", "반갑", "습니다." };

            foreach(string str in arr)
            {
                Console.Write(str);
            }
        }
    }
}
