using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ForApp001
{
    internal class Program
    {
        static void Main(string[] args)
        {
            //1~100까지 합을 출력

            int sum = 0;

            for(int i=0; i<100; i++)
            {
                sum += i+1;
            }
            Console.WriteLine(sum);
        }
    }
}
