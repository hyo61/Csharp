using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace While
{
    internal class Program
    {
        static void Main(string[] args)
        {
            // 1~200까지
            int i = 1;

            while (i <= 200)
            {
                Console.Write(i + " "); //Write : 옆으로 나열, WriteLine : 아래로 나열
                //i = i + 1
                //i += 1;
                i++; //후치연산자, ++i도 되고 식 간단함
            }
        }
    }
}
