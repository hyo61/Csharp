using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace if_num
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int num = Int32.Parse(Console.ReadLine());

            if(num > 89)
            {
                Console.WriteLine("A");
            }
            else if(num > 79)
            {             
                Console.WriteLine("B");
            }
            else if(num > 69)
            {
                Console.WriteLine("C");
            }
            else if(num > 59)
            {
                Console.WriteLine("D");
            }
            else
            {
                Console.WriteLine("F");
            }
        }
    }
}
